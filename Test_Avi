    <script>
      function initialize_flag() {
        // overlay related
        $(".toggle-flag-overlay").click(function(e) {
          e.preventDefault();
          $("#theatre-ia-wrap").removeClass('flagged');
        });

        // overlay - checkboxes
        $("#flag-checkboxes a").on('click', function(e) {
          e.preventDefault();
          $(this).children(".my-checkbox").toggleClass('checked');
          $.get($(this).attr('href'))
        });

                
       }
       $(initialize_flag());
    </script>
